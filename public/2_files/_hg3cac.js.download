try { 
	webengage.onReady(function () {
    webengage.notification.onOpen(function (data) {
        if (data.notificationId === '~10cb5989b' || data.notificationId === '~10cb598a2') {
            webengage.util.withWeJquery(function () {
                $weJQuery('#webklipper-publisher-widget-container-notification-frame').contents().find('.code').click(function (e) {
                    $weJQuery('#webklipper-publisher-widget-container-notification-frame').contents().find('.code').removeClass('offerCodeDisabled');
                    e.target.classList.add('offerCodeDisabled');
                    var el = e.target.innerText;
                    e.target.innerText = "COPIED";
                  
                    navigator.clipboard.writeText(el);
                });
            });
        }
    });
});
 } catch(e) { 
 	if (e instanceof Error) { 
		var data = e.stack || e.description;
		data = (data.length > 900 ? data.substring(0, 900) : data);
	 	webengage.eLog(null, 'error', data, 'cwc-error','cwc', '~hg3cac');
	 }
 }
